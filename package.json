    "jsbn": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.446.0",
    "next": "14.1.4",
    "next-themes": "^0.4.3",
    "paillier-bigint": "^3.4.3",
    "poseidon-lite": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-quill": "^2.0.0",
