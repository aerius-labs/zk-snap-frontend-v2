{
  "name": "zk-snap-frontend-v2",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm run lint --fix && pnpm run prettier && next dev",
    "build": "next build && pnpm run build:workers",
    "build-stats": "ANALYZE=true pnpm run build",
    "build:workers": "tsc -p tsconfig.worker.json",
    "watch:workers": "tsc -p tsconfig.worker.json --watch",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "prettier": "prettier --write --cache '**/**/*.{js,jsx,ts,tsx,css}'",
    "check-types": "tsc --noEmit --pretty",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache",
    "test:related": "jest --findRelatedTests",
    "test:staged": "jest --onlyChanged"
  },
  "dependencies": {
